# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TripMo' do
  use_frameworks!

	def project_pods
		pod ‘Quick’
		pod ‘Nimble’
		pod ‘RealmSwift’
	end
		

  # Pods for TripMo

  target 'TripMoTests' do
    inherit! :search_paths
    # Pods for testing
    project_pods
  end

  target 'TripMoUITests' do
    inherit! :search_paths
    # Pods for testing
    project_pods
  end

end
